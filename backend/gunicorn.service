[Unit]
Description=Gunicorn service
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/home/root/app
ExecStart=/home/root/app/venv/bin/gunicorn \
	--access-logfile - \
	--error-logfile - \
	--workers 3 \
	--bind unix:/home/root/app–∞/mycloud/mycloud.sock mycloud.wsgi:application


[Install]
WantedBy=multi-user.target
